        variables: { object_id, object_type },
      })

      var callback = (data) => {
        var content = (data != null && data.data != null && data.data.get_follow_summary != null && data.data.get_follow_summary.follower_count > 0) ? data.data.get_follow_summary.follower_count : "--";
        $(".followers-id").html(content);
      }
      makeGraphqlRequest(body, callback);
    }

    function userFollowEvent() {
      var query = `mutation follow($object_id: Int!, $object_type: String!) {
                            follow(object_id: $object_id, object_type: $object_type){
                                id
                            }
                        }`;
      var body = JSON.stringify({
        query,
        variables: { object_id, object_type },
      })

      var callback = (data) => {
        if (data != null && data != null && data.errors != null && data.errors[0].message == "Phiên làm việc hết hạn, vui lòng đăng nhập lại") {
          setTimeout(function () { window.location = '/sign-in?ref=' + encodeURI(window.location.href); }, 300);
          return;
        }
        setTimeout(() => {
          updateUIFollowEvent();
        }, 1000)
      }

      makeGraphqlRequest(body, callback);
    }

    function userUnFollowEvent() {
      var query = `mutation unfollow($object_id: Int!, $object_type: String!) {
                            unfollow(object_id: $object_id, object_type: $object_type){
                                id
                            }
                        }`;
      var body = JSON.stringify({
        query,
        variables: { object_id, object_type },
      })

      var callback = (data) => {
        if (data != null && data != null && data.errors != null && data.errors[0].message == "Phiên làm việc hết hạn, vui lòng đăng nhập lại") {
          setTimeout(function () { window.location = '/sign-in?ref=' + encodeURI(window.location.href); }, 300);
          return;
        }
        setTimeout(() => {
          updateUIFollowEvent();
        }, 1000)
      }
      makeGraphqlRequest(body, callback);
    }

    function makeGraphqlRequest(body, callback) {
      fetch(graphQLUrl, {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json',
          'Accept': 'application/json',
          "x-tb-access-token": getCookie("TBoxJWT")
        },
        body: body
      })
        .then(r => r.json())
        .then((data) => {
          callback(data);
        });
    }

    function showHideFollowBtn() {
      var query = `query get_relationship($object_id: Int!, $object_type: String!) {
                            get_relationship(object_id: $object_id, object_type: $object_type){
                                id
                            }
                        }`;
      var body = JSON.stringify({
        query,
        variables: { object_id, object_type },
      })

      var callback = (data) => {
        if (data != null && data.data != null && data.data.get_relationship != null) {
          $(".follow-event").each(function () {
            $(this).hide();
          });
          $(".followed-event").each(function () {
            $(this).show();
          });
        } else {
          $(".follow-event").each(function () {
            $(this).show();
          });
          $(".followed-event").each(function () {
            $(this).hide();
          });
        }
      }
      makeGraphqlRequest(body, callback);
    }

    function bindingFollowBtn() {
      $(".follow-event").each(function () {
        $(this).on('click', function () {
          userFollowEvent();
        });
      });
    }

    function bindingUnFollowBtn() {
      $(".followed-event").each(function () {
        $(this).on('click', function () {
          userUnFollowEvent();
        });
      });
    }

    function updateUIFollowEvent() {
      updateNumFollowers();
      showHideFollowBtn();
    }

    window.addEventListener('load', function () {
      updateUIFollowEvent();
      bindingFollowBtn();
      bindingUnFollowBtn();
    })
  </script>

